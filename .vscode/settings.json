{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
    "cSpell.words": [
        "Assinged",
        "Ssid"
    ],
    "editor.inlineSuggest.showToolbar": "onHover",
    "console-ninja.outputMode": "In View",
}